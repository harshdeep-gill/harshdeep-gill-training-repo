{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Form",
	"name": "quark/form",
	"description": "Add a form to your page.",
	"category": "layout",
	"keywords": [
		"form"
	],
	"textdomain": "qrk",
	"attributes": {
		"form": {
			"type": "string",
			"default": "none"
		},
		"thankYouPage": {
			"type": "object",
			"default": null
		}
	},
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false,
    "disableOnChinaSite": true
	}
}
