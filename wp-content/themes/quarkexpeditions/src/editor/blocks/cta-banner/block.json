{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "quark/cta-banner",
	"description": "Add description & CTA in a Banner",
	"title": "CTA Banner",
	"keywords": [ "banner", "cta" ],
	"category": "layout",
	"textdomain": "qrk",
	"attributes": {
		"backgroundImage": {
			"type": "object",
			"default": {}
		},
		"appearance": {
			"type": "string",
			"default": "light"
		},
		"backgroundColor": {
			"type": "string",
			"default": "gray"
		}
	},
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
