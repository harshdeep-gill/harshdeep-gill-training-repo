  "description": "Display related adventure options.",
  "category": "layout",
  "textdomain": "qrk",
  "keywords": [
    "related",
    "adventure",
    "options"
  ],
  "attributes": {
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "showDescription": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "alignWide": false,
    "anchor": true,
    "className": false,
    "html": false,
    "customClassName": false
  }
}
