{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Hero Title Bicolor",
	"name": "quark/hero-title-bicolor",
	"description": "Hero Title text with two colors.",
	"category": "widgets",
	"keywords": [
		"hero",
		"title",
		"text",
		"bicolor"
	],
	"textdomain": "qrk",
	"attributes": {
		"whiteText": {
			"type": "string",
			"default": ""
		},
		"yellowText": {
			"type": "string",
			"default": ""
		},
		"usePromoFont": {
			"type": "boolean",
			"default": false
		},
		"yellowFirst": {
			"type": "boolean",
			"default": false
		}
	},
	"parent": [
		"quark/hero-content-left"
	],
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
