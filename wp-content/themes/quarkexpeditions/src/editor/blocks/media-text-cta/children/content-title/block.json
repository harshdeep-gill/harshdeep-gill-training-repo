{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "quark/media-text-cta-content-title",
    "description": "Display a title in the media-text-cta block",
    "title": "Media Text CTA Content Title",
    "keywords": [
      "media",
      "text",
      "cta",
      "content",
      "title"
    ],
    "category": "layout",
    "textdomain": "quark",
    "parent": [
        "quark/media-text-cta"
    ],
    "attributes": {
      "title": {
        "type": "string",
        "default": ""
      },
      "headingLevel": {
        "type": "number",
        "default": 3,
        "enum": [ 2, 3, 4 ]
      }
    },
    "supports": {
      "alignWide": false,
      "anchor": true,
      "className": false,
      "html": false,
      "customClassName": false
    }
}
