{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Info Cards - Card",
	"name": "quark/info-card",
	"description": "Individual Info card item.",
	"parent": [
		"quark/info-cards"
	],
	"category": "layout",
	"keywords": [
		"info",
		"card",
		"item"
	],
	"textdomain": "qrk",
	"attributes": {
		"url": {
			"type": "object",
			"default": {}
		},
		"contentPosition": {
			"type": "string",
			"default": "bottom"
		},
		"image": {
			"type": "object",
			"default": {}
		}
	},
	"supports": {
		"alignWide": false,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
