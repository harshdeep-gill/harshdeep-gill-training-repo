{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Info Cards",
	"name": "quark/info-cards",
	"description": "Display a carousel of Info cards.",
	"category": "layout",
	"textdomain": "qrk",
	"keywords": [
		"info",
		"cards"
	],
	"attributes": {
		"layout": {
			"type": "string",
			"default": "grid"
		},
		"mobileCarousel": {
			"type": "boolean",
			"default": true
		},
		"carouselOverflow": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
