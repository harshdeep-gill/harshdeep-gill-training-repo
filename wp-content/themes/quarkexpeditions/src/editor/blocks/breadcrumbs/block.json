{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Breadcrumbs",
	"name": "quark/breadcrumbs",
	"description": "Display a Breadcrumbs block.",
	"category": "layout",
	"keywords": [
		"breadcrumbs"
	],
	"textdomain": "qrk",
	"attributes": {
		"textColor": {
			"type": "string",
			"default": "black",
			"enum": [
				"white",
				"black"
			]
		}
  },
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
