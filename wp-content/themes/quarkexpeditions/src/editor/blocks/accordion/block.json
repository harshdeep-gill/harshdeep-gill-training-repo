{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Accordion",
	"name": "quark/accordion",
	"description": "Add Accordion Block with one or more items",
	"category": "layout",
	"keywords": [
		"accordion"
	],
	"textdomain": "qrk",
	"attributes": {
		"hasBorder": {
			"type": "boolean",
			"default": true
		},
		"faqSchema": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
