{
  "schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Highlights",
  "name": "quark/highlights",
  "description": "Display a list of highlights with icons.",
  "category": "layout",
  "keywords": [
    "icon",
    "highlights"
  ],
  "textdomain": "qrk",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "info": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "alignWide": false,
    "anchor": true,
    "className": false,
    "html": false,
    "customClassName": false
  }
}
