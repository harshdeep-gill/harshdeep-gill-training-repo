{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Review Cards",
	"name": "quark/review-cards",
	"description": "Display a carousel of review cards.",
	"category": "layout",
	"textdomain": "qrk",
	"keywords": [
		"reviews",
		"cards"
	],
	"attributes": {
		"isCarousel": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
