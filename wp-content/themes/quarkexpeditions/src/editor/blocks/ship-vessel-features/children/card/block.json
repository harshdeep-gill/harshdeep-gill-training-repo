{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Ship Vessel Features Card",
	"name": "quark/ship-vessel-features-card",
	"description": "Display a card with a title, image, and description.",
	"category": "widgets",
	"keywords": [
		"ship",
		"vessel",
		"features",
		"card"
	],
	"textdomain": "qrk",
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"image": {
			"type": "object",
			"default": {}
		},
		"description": {
			"type": "string",
			"default": ""
		}
	},
	"parent": [
		"quark/ship-vessel-features"
	],
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
