{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "quark/lp-header",
	"description": "LP Header Block",
	"title": "LP Header",
	"category": "layout",
	"textdomain": "qrk",
	"keywords": [
		"lp",
		"header"
	],
	"attributes": {
		"tcImage": {
			"type": "object"
		},
		"ctaText": {
			"type": "string"
		},
		"ctaNumber": {
			"type": "string"
		},
		"darkMode": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"alignWide": false,
		"anchor": true,
		"className": false,
		"html": false,
		"customClassName": false
	}
}
